{
  "name": "Amazon Twitter Feed",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["*://www.amazon.com/","*://www.amazon.com/?*"],
      "js": ["main.bundle.js"]
    }
  ]
}
